{
    "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "permissions": {
    "allow": [
      "Read",
      "Edit",
      "Write",
      "Skill",
      "Bash",
      "WebSearch",
      "WebFetch"
    ],
    "deny": [
      "Read(.env*)",
      "Write(.env*)",
      "Bash(rm -rf:*)"
    ],
    "defaultMode": "acceptEdits"
  }
}
